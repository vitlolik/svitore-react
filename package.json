{
	"name": "svitore-react",
	"description": "React binding for svitore",
	"version": "0.0.3-0",
	"type": "module",
	"files": [
		"dist/*"
	],
	"main": "./dist/index.umd.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"start": "vite",
		"build": "vite build && tsc -p tsconfig.build.json",
		"preversion": "npm run build",
		"lint": "eslint 'src/**/*.ts' 'demo/**/*.ts'"
	},
	"peerDependencies": {
		"react": "^18",
		"svitore": "*"
	},
	"devDependencies": {
		"@types/node": "^18.15.11",
		"@types/react": "^18.0.37",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"@vitejs/plugin-react": "^3.1.0",
		"@vitest/coverage-c8": "^0.30.1",
		"eslint": "^8.38.0",
		"eslint-plugin-react": "^7.32.2",
		"react": "^18",
		"react-dom": "^18.2.0",
		"svitore": "^0.0.3-0",
		"typescript": "^5.0.4",
		"vite": "^4.2.2"
	},
	"keywords": [
		"svitore",
		"svitore-react"
	],
	"repository": "https://github.com/vitlolik/svitore-react",
	"license": "MIT"
}
